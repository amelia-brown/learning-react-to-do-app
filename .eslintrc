{
  "env": {
      "browser": true,
      "node": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
          "jsx": true,
          "experimentalObjectRestSpread": true
      },
  },
  "plugins": [
    "react"
  ],
  "extends": "plugin:react/recommended"
}

